    .scope S_0x12ae04870;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600001a18090_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x12ae04870;
T_1 ;
    %wait E_0x600003d18240;
    %load/vec4 v0x600001a18090_0;
    %inv;
    %assign/vec4 v0x600001a18090_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x12ae04700;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600001a181b0_0, 0, 1;
    %end;
