fn Main() void {
  BREAKPOINT;
  var vec: Vec;
  vec.x = 7;
  vec.y = 8;
  vec.z = vec.x;

  var node: Node;
  var ptr: Node* = node.&;
  node.self = ptr;
  node.vec = vec;
  
  BREAKPOINT;
  -node;
  
  @nop();
  @nop();
  @nop();
  @nop();
  @nop();
  @susp();
}


struct Vec {
  x: u16,
  y: u32,
  z: u16,
}

struct Node {
  vec: Vec,
  self: Node*,
}