#include malloc
#include renderfile

fn Main() void {
    var memory: Block[MALLOC_SIZE];
    memory:MemInit();

    var rs: RenderState;
    rs.&:Init();

    rs.file = "Hello World!";

    rs.&:Draw();

    WriteCharFlaggedColored('A', 0, 0, 0, 0xFFFFFF, 0x000000);

    while (true) {}
    
    return;
}